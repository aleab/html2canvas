{
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": 3,
                "loose": true,
                "modules": false,
                "targets": {
                    "browsers": "> 0.2%, last 2 versions, IE > 8"
                },
                "useBuiltIns": "usage"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "add-module-exports",
        ["const-enum", {"transform": "constObject"}]
    ]
}
