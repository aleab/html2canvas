{
    "title": "html2canvas",
    "name": "html2canvas",
    "description": "Screenshots with JavaScript",
    "main": "dist/lib/index.js",
    "module": "dist/lib-esm/index.js",
    "browser": "dist/html2canvas.js",
    "typings": "dist/types/index.d.ts",
    "version": "1.0.0-rc.7",
    "author": {
        "name": "Niklas von Hertzen",
        "email": "niklasvh@gmail.com",
        "url": "https://hertzen.com"
    },
    "homepage": "https://html2canvas.hertzen.com",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:niklasvh/html2canvas.git"
    },
    "bugs": {
        "url": "https://github.com/niklasvh/html2canvas/issues"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "scripts": {
        "prepack": "npm install && npm run build",
        "prebuild": "rimraf dist && rimraf build && rimraf background-parser/dist && mkdirp build",
        "postbuild": "npm run build:create-reftest-list && npm run build:testrunner",
        "build": "tsc -m commonjs --outdir dist/lib && tsc -m es6 --outdir dist/lib-esm && npm run build:background-parser && npm run build:rollup",
        "build:background-parser": "tsc -p background-parser -m commonjs && tsc -p background-parser -m es6 --outdir background-parser/dist/lib-esm",
        "build:rollup": "rollup -c rollup.config.ts",
        "build:testrunner": "rollup -c tests/rollup.config.ts",
        "build:reftest-result-list": "ts-node scripts/create-reftest-result-list.ts",
        "build:create-reftest-list": "ts-node scripts/create-reftest-list.ts tests/reftests/ignore.txt build/reftests.ts",
        "build:reftest-preview": "webpack --config www/webpack.config.js",
        "release": "standard-version",
        "format": "prettier --write \"{src,www/src,tests,scripts}/**/*.ts\"",
        "lint": "eslint src/**/*.ts",
        "test": "npm run lint && npm run unittest && npm run karma",
        "unittest": "jest",
        "karma": "node karma",
        "watch": "rollup -c rollup.config.ts -w",
        "watch:unittest": "mocha --require ts-node/register --watch-extensions ts -w src/**/__tests__/*.ts",
        "start": "node tests/server"
    },
    "dependencies": {
        "css-line-break": "^1.1.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-typescript": "^7.9.0",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-json": "^4.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@types/chai": "^4.2.11",
        "@types/glob": "^7.1.1",
        "@types/jest": "^25.2.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/platform": "^1.3.2",
        "@types/promise-polyfill": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "appium-ios-simulator": "^3.21.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-add-module-exports": "^1.0.2",
        "babel-plugin-const-enum": "0.0.5",
        "babel-plugin-dev-expression": "^0.2.2",
        "base64-arraybuffer": "0.2.0",
        "body-parser": "^1.19.0",
        "chai": "^4.2.0",
        "chromeless": "^1.5.2",
        "cors": "^2.8.5",
        "es6-promise": "^4.2.8",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "express": "^4.17.1",
        "filenamify-url": "^2.1.1",
        "glob": "^7.1.6",
        "html2canvas-proxy": "1.0.1",
        "jest": "^25.2.7",
        "jquery": "^3.4.1",
        "js-polyfills": "^0.1.42",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-edge-launcher": "^0.4.2",
        "karma-firefox-launcher": "^1.3.0",
        "karma-ie-launcher": "^1.0.0",
        "karma-junit-reporter": "^2.0.1",
        "karma-mocha": "^1.3.0",
        "karma-safari-launcher": "^1.0.0",
        "karma-sauce-launcher": "^4.1.2",
        "mocha": "^7.1.1",
        "node-simctl": "^6.1.0",
        "platform": "^1.3.5",
        "prettier": "^2.0.4",
        "replace-in-file": "^5.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.3.3",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-sourcemaps": "^0.5.0",
        "rollup-plugin-terser": "^5.3.0",
        "serve-index": "^1.9.1",
        "slash": "^3.0.0",
        "standard-version": "^7.1.0",
        "ts-jest": "^25.3.1",
        "ts-loader": "^6.2.2",
        "ts-node": "^8.8.2",
        "tslib": "^1.11.1",
        "typescript": "^3.8.3",
        "uglify-js": "^3.8.1",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11"
    }
}
